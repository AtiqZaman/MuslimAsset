<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.atiq.MuslimAsset">

    <application
        android:name="com.atiq.MuslimAsset.App"
        android:allowBackup="true"
        android:configChanges="keyboardHidden|orientation|screenSize"
        android:fullBackupContent="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.Mushaf">
        <activity

            android:name="com.atiq.MuslimAsset.mainActivities.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.atiq.MuslimAsset.mainActivities.AyatActivity"
            android:parentActivityName="com.atiq.MuslimAsset.mainActivities.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.atiq.MuslimAsset.mainActivities.MainActivity" />
        </activity>



        <activity android:name=".ActivityHome"></activity>

        <activity android:name=".ActivityHadith"></activity>
        <activity android:name=".ActivityPrayer"></activity>
        <activity android:name=".ActivityMore"></activity>




    </application>

</manifest>
